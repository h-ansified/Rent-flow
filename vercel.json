{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/client/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "installCommand": "npm install"
}